#>A.ulam
  ulam 3;
element A {
  String m_s;
  Int m_total;
 Int test() {
   SystemU3 su3;
   String mt = "";
   m_s = mt;
   su3.print(m_s);
   m_total += m_s.lengthof;
   su3.print(m_total);
   su3.assert(m_total == 0);

   String s; //uninitialized value, s
   s = "hello world";
   m_s = s;
   su3.print(m_s);
   m_total += m_s.lengthof;

   su3.print(m_total);
   su3.assert(m_total == 11);
   return m_total;
 }
}

