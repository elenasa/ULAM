element T{
  Int test(){
    System sys;

    E(true) et;

    sys.print(et.csi);
    sys.assert(et.csi == 2);
    sys.print(et.b);
    sys.assert(et.b); //true


    //second test just to make sure
    E(false) ef;
    sys.print(ef.csi);
    sys.assert(ef.csi == 2);
    sys.print(ef.b);
    sys.assert(ef.b); //true

    return et.csi; //2
  }
}

