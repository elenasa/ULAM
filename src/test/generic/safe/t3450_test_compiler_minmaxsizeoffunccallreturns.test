#>T.ulam
  ulam 2;
element T{
  typedef Int(7) B;
  B m,n,p,q;
  B func1() {
    return 5;
  }
  B func2() {
    return 7;
  }
  String func3()
  {
    return "hello world";
  }

  Int test(){
    System s;
    m = func1().minof;
    s.print((Int) m);
    s.assert(m == -64);
    n = func2().maxof;
    s.print((Int) n);
    s.assert(n == 63);
    p = func1().sizeof;
    s.print((Int) p);
    s.assert(p == 7);
    q = func3().lengthof;
    s.print((Int) q);
    s.assert(q == 11);
    return func1().sizeof;
  }
}

