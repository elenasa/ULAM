## "conditional downcast"
## informed by 41344, except implicit 'self' in as-block used with function calls
##
#:Typo.ulam
  ulam 4;
use Eltypo;
quark Typo(Int a) : Bar{
  Bool typool;
  Void functest()
  {
    System s;
    if(self as Eltypo){
      super.typool = true; //super optional, found in Eltypo ancestor
      s.assert(typool);
      t.func(funcel()); //modifies ElTypo datamember 'bu' in Bar
      s.print(t.bu);
      s.assert(t.bu == funcel());
      first = t.bu; //2u, from 7u
    }
    else
      s.assert(false);
  }

  virtual Unsigned(3) funcel()
  {
    return 5u;
  }
}

#:Bar.ulam
  ulam 2;
quark Bar
{
  Unsigned(3) bu = 7u;
  Void func(Unsigned u)
  {
    bu = (Unsigned(3)) u;
  }

  virtual Unsigned(3) funcel()
  {
    return 3u;
  }
}

#>Eltypo.ulam
  ulam 2;
use Typo;
use System;
element Eltypo : Typo(1) {
  System s;
  constant Unsigned(3) cTWO = 2u;
  Unsigned(3) first;
  Unsigned(3) second;
  Typo(1) t;
  Int test(){
    functest();
    s.print(t.bu);
    s.assert(t.bu == cTWO);
    s.print(first);
    s.assert(first == cTWO);
    second = self.bu; //still 7
    return second;
  }

  virtual Unsigned(3) funcel()
  {
    return cTWO;
  }

}

