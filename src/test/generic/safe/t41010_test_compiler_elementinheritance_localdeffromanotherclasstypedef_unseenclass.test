##
## test of inheritance: uses localdef typedef from another class, informed by t3862, t41009
##
#>R.ulam
  ulam 3;
element R {
  Int test() {
    System s;
    Foo f;
    f.a = f.us;
    s.print(f.a);
    s.assert(f.a == 4u);
    f.a = Constants.Soo3.sizeof;
    s.print(f.a);
    s.assert(f.a == 3u);

    f.sp = f.func();
    s.assert(f.sp);
    return (Int) f.a; //=3
  }
}

#:Foo.ulam
  ulam 3;
local typedef Constants.Soo3 Soo3; //order counts ):
element Foo : local.Soo3 {
    Bool(7) sp;
    Unsigned a = 73;
}

#:Constants.ulam
  ulam 1;
quark Constants {
  constant Unsigned(3) soosize = 3u;
  typedef Soo(soosize) Soo3;
}

#:Soo.ulam
  ulam 3;
quark Soo(Unsigned(3) v) {
  Unsigned(v) us = 4u;
  Bool check()
  {
    return us.sizeof == Constants.soosize;
  }
  Bool func()
  {
    if(self is Constants.Soo3)
      return check();
    return false;
  }
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}
