#>Foo.ulam
  ulam 3;
element Foo
{
  Unsigned & func(Unsigned& arg1, Unsigned& arg2, Bool barg)
  {
    if(barg)
      return arg1;
    return arg2;
  }

  Void caller1()
  {
    Unsigned f1 = 2;
    caller2(f1, false);
  }

  Unsigned & caller2(Unsigned& a1, Bool barg)
  {
    Unsigned f2 = 3;
    Unsigned & r2 = f2;
    return func(r2, a1, barg);  // Safe or not?  Can't know til runtime.
  }

  Int test() {
    System s;
    caller1();

    Unsigned f3 = 4;
    Unsigned & r = caller2(f3, false);
    s.print(r);
    s.assert(r == 4u);
    return (Int) r;
  }
}

