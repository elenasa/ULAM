#>Foo.ulam
  ulam 1;
use System;
element Foo {
  System s;
  Bool(3) m_b;
  Bool(3) check(Bool(2+1) b) {
    return b /* true */;
  }
  Bool(3) check(Bool(2+3) b) {
    return false;
  }
  Int test() {
    m_b = check( (Bool(3)) true);
    s.print(m_b);
    return (Int) m_b;
  }
}
