#>Foo.ulam
  ulam 3;
element Foo
{
  Int(3) m_i = 3;

  Foo & func(Foo& arg1, Foo& arg2, Bool barg)
  {
    if(barg)
      return arg1;
    return arg2;
  }

  Void caller1()
  {
    Foo f1;
    caller2(f1, false);
  }

  Foo & caller2(Foo& a1, Bool barg)
  {
    Foo f2;
    Foo & r2 = f2;
    return func(r2, a1, barg);  // Safe or not?  Can't know til runtime.
  }

  Int test() {
    System s;
    caller1();
    Foo f3;
    f3.m_i = 1;
    Foo & r = caller2(f3, false);
    //Foo & t = caller2(f3, true); //see fail/t41030
    s.print(r.m_i);
    s.assert(r.m_i == 1);
    return r.m_i;
  }
}

