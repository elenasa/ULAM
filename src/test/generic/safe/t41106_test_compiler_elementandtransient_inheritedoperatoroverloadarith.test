##
## informed by t41104; test of inherited operator+ for an element and transient
##
#>Foo.ulam
  ulam 3;
element Foo : QW {
  Bool first;
  Int opfint;
  Int test() {
    System s;
    Foo foo;
    foo.qint = 2;

    //inherited overloaded operator +
    opfint = foo + 3;
    s.print(opfint);
    s.assert(opfint == 5);

    Too too;
    too.qint = 3;
    //inherited overloaded operator +
    too.topint = too + 4;
    s.print(too.topint);
    s.assert(too.topint == 7);

    return opfint;
  }
}

