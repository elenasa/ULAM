##
## suggested by Dave 4/15/17 for ?: test;
##
#>A.ulam
  ulam 3;
element A {
  Bool ok;
  Int(3) a[4];
  Int test() {
    System s;
    Int(3) b[4];

    //with parens, local and data member
    (ok ? a[1] : b[2]) = 1;
    s.print(b[2]);
    s.assert(b[2]==1);

    //without parens, data member and local
    ok = true;
    ok ? a[3] : b[0] = 2;
    s.print(a[3]);
    s.assert(a[3]==2);

    //custom array, local
    Qbar qb;
    ok ? qb[0] : qb[1] = 3;
    s.print((Int) qb[0]);
    s.assert(qb[0] == 3);

    return 0;
  }
  Int d;
}

