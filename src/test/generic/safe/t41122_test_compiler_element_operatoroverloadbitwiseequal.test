##
## first test to parse bitwise equal for an element
## informed by t41115.
##
#>Foo.ulam
  ulam 3;
element Foo {
  Bool first;
  Bits m_it;
  Int(4) m_i;
  Int test() {
    System s;
    Foo f;

    f |= 5u;
    s.print((Unsigned) f.m_it); //prints 5
    s.assert(f.m_it == 0x5);

    f &= 3u;
    s.print((Unsigned) f.m_it); //prints 1
    s.assert(f.m_it == 0x1);

    f ^= 5u;
    s.print((Unsigned) f.m_it); //prints 4
    s.assert(f.m_it == 0x4);

    return (Int) f.m_it;
  }

  //7c3d
  Void operator|=(Unsigned arg)
  {
    m_it |= arg;
  }

  //263d
  Void operator&=(Unsigned arg)
  {
    m_it &= arg;
  }

  //5e3d
  Void operator^=(Unsigned arg)
  {
    m_it ^= arg;
  }

}

