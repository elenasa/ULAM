##
##      // includes statements after embedded block; use of variable
##      // inside block defined outside then redefined inside;
##
#>A.ulam
  ulam 1;
use System;
element A{
  System s;
  Int j;
  Int test () {
    constant Int cOW = 3;
    j = 1 + cOW;
    s.print(j);
    {
      j = j + cOW;
      s.print(j);
      Int j;
      constant Int cOW = 4;
      j = 2 * cOW;
      s.print(j);
    }
    i = j;
    s.print(i);
    return j;
  }
  Int i;
}
