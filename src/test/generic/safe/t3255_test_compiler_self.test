 ulam 1;
use System;
element S{
  System s;
  Bool sp;
  Bool(3) b1, b2;
  Bool func() {
    return (self is S);
  }
  Bool func(Atom d) {
    return (d is S);
  }
  Int test() {
    Atom a, t;
    a = self;
    if(a as S)
      b2 = a.func();
    s.print(b2);
    s.assert(b2); //true
    b1 = func(a);
    s.print(b1);
    s.assert(b1); //true
    b1 = func(t);
    s.print(b1);
    s.assert(!b1); //false
    return (Int) b2;
  }
}
