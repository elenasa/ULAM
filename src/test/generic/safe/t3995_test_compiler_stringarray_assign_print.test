##
## Tests propagtion of initialized string array
##
#>A.ulam
  ulam 3;
element A {
  constant String cStrArr[4] = { "one", "two", "three"}; //propagates

  Int m_total;
  Int test() {
    SystemU3 su3;
    su3.print(cStrArr[3]);

    for(Int i = 0; i < cStrArr.lengthof; i++)
    {
      m_total += cStrArr[i].lengthof;
    }

    su3.print(m_total);
    su3.assert(m_total == 16);

    return m_total;
 }
}

