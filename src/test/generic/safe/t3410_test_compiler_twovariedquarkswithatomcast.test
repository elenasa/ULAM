element Foo {
  IntXY(1,4) q;
  IntXY(3u,4u) n;
  IntXY(2,3) p;
  Int test() {
    System s;

    //make into local quark
    IntXY(3u,4u) m = n.make(1,2);
    s.print(m.x);
    s.assert(m.x == 1);
    s.print(m.y);
    s.assert(m.y == 2);
    s.print(n.x);
    s.assert(n.x == 0);
    s.print(n.y);
    s.assert(n.y == 0);

