    //s.print(n.x);
    //s.assert(n.x == 2);
    //s.print(n.y);
    //s.assert(n.y == 3);

    //make local element dm from local var quark
    Foo f;
    f.n = m.make(4,1);
    s.print(f.n.x);
    s.assert(f.n.x == 4);
    s.print(f.n.y);
    s.assert(f.n.y == 1);
    s.print(m.x);
    s.assert(m.x == 1);
    s.print(m.y);
