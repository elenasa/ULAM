#:Q.ulam
  ulam 5;
transient Q {
  constant Unsigned cSIZE = 66u;
  constant Unsigned cHALF = cSIZE/2u;
  typedef Bits(cSIZE) CBits;
  CBits mBits;

  Void func() {
    SystemU3 du;
    mBits = 0u;
    CBits v;
    v = ((CBits) 1u)<<cHALF;
    mBits |= v;
    if (mBits != 0u)
      du.print("nonzero");
    else
      du.print("zero");
    mBits &= v;
    if (mBits != 0u)
      du.print("still nonzero");
    else
      du.print("still zero");
    mBits ^= v;
    if (mBits != 0u)
      du.print("now nonzero");
    else
      du.print("now zero");

    v = 0;
    v = ((CBits) 1u)<<cHALF;
    du.print((Unsigned) v); //65536 cSIZE==32
    v = v >> cHALF;
    du.print((Unsigned) v); //1
    if (v != 0u)
      du.print("v now nonzero");
    else
      du.print("v now zero");

    if (v == 1u)
      du.print("v now one");
    else
      du.print("v not one");


  }
}

#:SystemU3.ulam
  ulam 3;
quark SystemU3 : System {
  Void print(String s) native;
}

#:System.ulam
  ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}

