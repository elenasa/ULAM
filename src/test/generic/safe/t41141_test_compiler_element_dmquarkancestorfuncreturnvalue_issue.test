#>MyElement.ulam
  ulam 3;
element MyElement
{
  Bool sp;
  MySub mSub;
  Int test() {
    System s;
    mSub.sppp = 7u;
    mSub.spq = 1u;
    s.print(mSub.spq);
    s.assert(mSub.spq == 1u);

    MyQuark & qref = func();
    s.print(qref.spq);
    s.assert(qref.spq == 1u);

    //effective self of DN is still MySub:
    s.assert(qref is MySub);
    return 0;
  }

  MyQuark & func() { return mSub; }
}

#:MyQuark.ulam
  ulam 3;
quark MyQuark {
  Unsigned(1) spq;
}

#:MySub.ulam
  ulam 3;
quark MySub : MyQuark {
  Unsigned(3) sppp;
}

#:System.ulam
