#:R.ulam
  ulam 3;
local constant T cet = { .m_sp = true, .m_i = 99 };
element R{
  Int m_ri;

  Int func(constant T& targ) {
    m_ri = targ.m_i;
    return m_ri;
  }

  Int test(){
    System sys;

    T t = cet; //see gen code
    Int it = func(t);
    sys.print(it);
    sys.assert(it == 99);

    Int i = func(cet);
    sys.print(i);
    sys.assert(i == 99);

    return m_ri;
  }
}

