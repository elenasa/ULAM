## test inherited overload operator equal for an element using refs
## informed by t41120.
##
#>Foo.ulam
  ulam 3;
element Foo : QW {
  Int fint, opfint;
  Int test() {
    System s;
    Foo f,g;
    f.m_i = 4;
    QW& gqref = g;
    QW& fqref = f;
    gqref = fqref;
    s.assert(!g.first);
    s.print(g.m_i); //prints 4
    s.assert(f.m_i == g.m_i);
    return f.m_i;
  }
}

