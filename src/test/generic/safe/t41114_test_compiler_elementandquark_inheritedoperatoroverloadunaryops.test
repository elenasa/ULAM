#>Foo.ulam
  ulam 3;
element Foo : QW {
  Bool first;
  Int fint, opfint;
  Int test() {
    System s;

    QW qw;
    Int qint = qw + qw; //lhs overload +, rhs uses toInt
    s.print(qint);
    s.assert(qint == 4);

    Int qint2 = + qw; //overload +, not toInt!!
    s.print(qint2);
    s.assert(qint2 == 5);

    Foo foo;
    foo.fint = 2;

    //overloaded unary operator+ (2b), inherited
    opfint = +foo;
    s.print(opfint);
    s.assert(opfint == 5);

    //overloaded unary operator- (2d), inherited
    opfint = -foo;
    s.print(opfint);
    s.assert(opfint == -1);

    //overloaded unary operator! (21), NOT inherited
    first = !foo;
    s.print(first);
    s.assert(first);

    //explicit call to unary operator+, inherited
    opfint = foo.operator+();
    s.print(opfint);
    s.assert(opfint == 5);
    return opfint;
  }

  Bool operator!()
  {
    return !first;
  }

}

#:QW.ulam
  ulam 3;
quark QW {
  Int operator+()
  {
    return 5;
  }

  Int operator-()
  {
    return -1;
  }

  Bool operator!()
  {
    return false;
  }

  Int toInt()
  {
    return 3;
  }

  Int operator+(Int iarg)
  {
    return 1 + iarg;
  }

}

