##
## informed by t41016: un-nested switch does not need breaks! no fall through. default optional.
##
#>A.ulam
  ulam 3;
element A {
  System s;
  Int a, b;
  Int test() {
    a = 1;
    b = 0;
    which((--a))
    {
    case 0:
      {
	b = 2; //hits, no fall through
      }
    case -1:
      {
	b = 3;
      }
      //    default: optional
    }
    s.print(b);
    s.assert(b == 2);
    s.print(a);
    s.assert(a == 0);
    return a;
  }
}

