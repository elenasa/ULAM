#:QMemBase.ulam
  ulam 2;
quark QMemBase {
  Int(3) bmem;
  Int func() {
    return vfunc();
  }
  virtual Int vfunc() {
    bmem = 1;
    return 3;
  }
}

#:QSubBase1.ulam
  ulam 2;
use QMemBase;
quark QSubBase1 : QMemBase { }

#:QSubBase2.ulam
  ulam 2;
use QMemBase;
quark QSubBase2 : QMemBase {
  virtual Int vfunc() {
    bmem = 2;
    return 4;
  }
}

