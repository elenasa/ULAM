#>Foo.ulam
  ulam 2;
use Soo;
use System;
element Foo {
  System s;
  Bool first;
  Soo su;
  Int test() {
    Foo f;
    first = f.su.check();
    if(first)
    {
      s.print(first);
      return Foo.sizeof;
    }
    return 0;
  }
}

