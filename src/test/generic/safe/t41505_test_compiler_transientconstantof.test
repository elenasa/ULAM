#>R.ulam
  ulam 2;
use TPerc;
use System;
element R {
  Int test() {
    System s;
    TPerc t;
    t.bb = true;
    t.set(7);
    s.assert(t.bb);
    s.assert(t.get() == 7);

    TPerc& tref = t;
    s.assert(tref.bb);
    s.assert(tref.get() == 7);

    TPerc b = TPerc.constantof; //not a lhs ref (see t3659)
    s.assert(b.bb != t.bb);
    s.assert(b.get() != t.get());
    return b.get();
  }
}

