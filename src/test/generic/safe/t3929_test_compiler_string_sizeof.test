##
## sizeof String is the number of bits occupied in an atom
##
#>A.ulam
  ulam 3;
element A {
  Int(8) m_s1;
  Int(8) m_s2;
  Int(8) m_s3;
 Int test() {
   System s;

   String mt = "";
   m_s1 = mt.sizeof; // =20
   s.print(m_s1);
   s.assert(m_s1 == 20);

   String str = "foo";
   m_s2 = str.sizeof; // =20
   s.print(m_s2);
   s.assert(m_s2 == 20);

   m_s3 = String.sizeof; //=20
   s.print(m_s3);
   s.assert(m_s3 == 20);

   return 0;
 }
}

