#:Typo.ulam
  ulam 5;
use Bar;
quark Typo(Int a) : Bar{
  Bool(3) spt;
  virtual Void func(Unsigned u)
  {
    spt = true;
    //super.func(u + 1u); //should be equivalent!
    self.Bar.func(u + 1u);
  }
}

#:Bar.ulam
  ulam 2;
quark Bar
{
  Unsigned(3) bu = 7u;
  virtual Void func(Unsigned u)
  {
    bu = (Unsigned(3)) u;
  }
}

#>Eltypo.ulam
  ulam 5;
use Typo;
use System;
element Eltypo : Typo(1) + Bar {
  Unsigned(3) first;
  Unsigned(3) second;
  Typo(1) t;

  Int test(){
    System s;
    Eltypo elf;
    Bar& barref = elf.t; //effself is Typo

    barref.func(3u);
    s.print(barref.bu);
    s.assert(barref.bu == 4);
    first = barref.bu; //4u, from 7u

    s.assert(elf.t.spt);
    s.assert(!elf.spt);

    s.print(elf.bu);
    s.assert(elf.bu == 7); //no change
    s.print(first);
    s.assert(first == 4);
    second = elf.t.bu; //4u, from 7u
    s.assert(second == 4u);
    return second;
  }
}

#:System.ulam
  ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}
