#>BS.ulam
  ulam 5;
local typedef Unsigned(5) Uint;

quark T0 {
  Uint mT0[2] = {0,1};
}
quark T1 : T0 {
  T0 mT1_T0;
  Uint mT1[1] = {3};
}
transient T2 : T1 + T0 {
  T1 mT2_T1;
  T0 mT2_T0;
  Uint mT2[5] = { 7,8,9,10,11};
}
transient T3 : T2 + T0 + T1 {
  T2 mT3_T2;
  T1 mT3_T1;
  T0 mT3_T0;
  Uint mT3[6] = { 12, 13, 14, 15, 16, 17 };
}

element BS +System {

  Int test()
  {
    T3 wow;
    constant Uint cT0[2] = {7,6};
    wow.mT0 = cT0; //modify base dm
    print(wow.mT0[0]);
    assert(wow.mT0[0] == 7); //changed

    if(wow as T1)
    {
      T1 t1 = wow;
      t1.mT0[1] = 3;
      t1.mT1[0] = 8;
      wow = t1; //T1 & T0 bases change
      assert(wow.mT0[1] == 3);
      assert(wow.mT1[0] == 8);
    }
    assert(wow.mT0[1] == 3); //also changed?
    assert(wow.mT1[0] == 8); //also changed?

    print(wow.mT0[0]); //separator

    if(wow as T2)
    {
      T2 t2 = wow;
      t2.mT0[0] = 1;
      t2.mT2[1] = 4;
      t2.mT1[0] = 11;
      wow = t2; //T2,T1,T0 bases change?
      assert(wow.mT0[0] == 1);
      assert(wow.mT2[1] == 4);
      assert(wow.mT1[0] == 11);
    }
    assert(wow.mT0[0] == 1); //also changed?
    assert(wow.mT2[1] == 4);
    assert(wow.mT1[0] == 11);

    return 0;
  }
}

