##
## test of local defs: different file scopes, shadowing locals, informed by t3858
##
#>Foo.ulam
  ulam 2;
local constant Unsigned(3) soosize = 3u;
local typedef Soo(soosize) Soo3;
element Foo : Soo3 {
  Bool(7) sp;
  Unsigned a = 73;
  Int test() {
    System s;
    Foo f;
    a = f.us;
    s.print(a);
    s.assert(a == 4u);
    a = Soo3.sizeof;
    s.print(a);
    s.assert(a == 3u);

    sp = func();
    s.assert(!sp);
    return (Int) a; //=3
  }
}

#:Soo.ulam
  ulam 2;
local constant Unsigned(3) soosize = 7u;
local typedef Soo(soosize) Soo3;
quark Soo(Unsigned(3) v) {
  Unsigned(v) us = 4u;
  Bool check()
  {
    return us.sizeof < soosize; //uses its local constant
  }
  Bool func()
  {
    if(self is Soo3) //uses its local typedef
      return check();
    return false;
  }
}

