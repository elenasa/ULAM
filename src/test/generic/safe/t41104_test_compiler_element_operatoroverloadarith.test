##
## first test to parse operator+,-.*,/,% for an element
##
#>Foo.ulam
  ulam 3;
element Foo {
  Bool first;
  Int fint, opfint;
  Int test() {
    System s;
    Foo foo;
    foo.fint = 2;

    //overloaded operator+ (2b)
    opfint = foo + 3;
    s.print(opfint);
    s.assert(opfint == 5);

    //overloaded operator- (2d)
    opfint = foo - 3;
    s.print(opfint);
    s.assert(opfint == -1);

    //overloaded operator* (2a)
    opfint = foo * 4;
    s.print(opfint);
    s.assert(opfint == 8);

    //overloaded operator/ (2f)
    opfint = foo / 2;
    s.print(opfint);
    s.assert(opfint == 1);

    //explicit call operator+
    opfint = foo.operator+(3);
    s.print(opfint);
    s.assert(opfint == 5);

    //overloaded operator% (?)
    opfint = foo % 3;
    s.print(opfint);
    s.assert(opfint == 2);

    return opfint;
  }

  Int operator+(Int iarg)
  {
    return fint + iarg;
  }

  Int operator-(Int iarg)
  {
    return fint - iarg;
  }

  Int operator*(Int iarg)
  {
    return fint * iarg;
  }

  Int operator/(Int iarg)
  {
    return fint / iarg;
  }

  Int operator%(Int iarg)
  {
    return fint % iarg;
  }
}

