#>Foo.ulam
  ulam 2;
use Too;
use System;
element Foo {
  Bool sp;
  Int ta = 9;
  QW qarr[4]; //quark array in element
  Int test() {
    System s;
    Too t;

    t.set(t.sizeof);
    ta = t.get();
    s.print(ta);
    t.sp = (ta == 881u);
    s.assert(t.sp);

    //read/write dm transient
    Tumi& tumiref = t.mis;
    tumiref.u2 = 9u;
    s.print(t.mis.u2);
    s.assert(t.mis.u2 == 9u);

    Tumi newtumi;
    newtumi.u2 = 8u;
    s.print(newtumi.u2);
    s.assert(newtumi.u2 == 8u); //not default

    t.mis = newtumi; //***the change
    s.print(t.mis.u2);
    s.assert(t.mis.u2 == 8u);

    return ta;
  }
}

