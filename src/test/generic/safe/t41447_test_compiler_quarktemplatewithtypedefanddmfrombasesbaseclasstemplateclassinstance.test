#:EventWindow.ulam
  ulam 3;
quark EventWindow {
    typedef Unsigned(6) SiteNum;
    typedef Unsigned(3) Symmetry;

    Symmetry changeSymmetry(Symmetry newSym){
      return newSym--;
    }

  Atom& aref(Int index) native;
}

