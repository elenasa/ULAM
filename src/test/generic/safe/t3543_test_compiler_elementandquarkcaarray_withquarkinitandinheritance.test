#>Foo.ulam
  ulam 2;
use System;
use Car;
element Foo {
  typedef Car Pop[2];
  System s;
  Bool(3) b, c;
  Pop m_bar2;
  Car sbar;
  Bool check(Int v) {
    return true;
  }
  Int test() {
    Unary(1) i = 1u;

    b = m_bar2[i].cb; //read case

    s.print(b); //true, should be
    s.assert(b);

    m_bar2[i].cb = false; //write case

    c = m_bar2[i].cb; //read case

    s.print(c); //false
    s.assert(!c);
    return 0;
  }
}

#:Car.ulam
  ulam 2;
use Bar;
quark Car : Bar {
  Bool cb = true;
}

#:Bar.ulam
