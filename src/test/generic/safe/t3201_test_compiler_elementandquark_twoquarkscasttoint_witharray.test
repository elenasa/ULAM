Uq_System { <NOMAIN> }
Uq_Bar { Bool(1) b(false);  Unsigned(3) x(0);  Unsigned(3) y(0);  <NOMAIN> }
#>Foo.ulam
  ulam 1;
use System;
use Bar;
element Foo {
  System s;
  Bar bar1;
  Bar bar2;
  Int(4) i[2];
  Int test(){
    Int(2) d;
    d = 1;
    bar1.x = (Unsigned(3)) d;
    bar1.y = 2;
    bar2.x = 3;
    bar2.y = 0;
    i[0] = (Int(4)) bar1;
    i[1] = (Int(4)) bar2;
    s.print(bar1.x);
    s.assert(bar1.x == 1);
    s.print(bar1.y);
    s.assert(bar1.y == 2);
    s.print(bar2.x);
    s.assert(bar2.x == 3);
    s.print(bar2.y);
    s.assert(bar2.y == 0);

    s.print(i[0]);
    s.assert(i[0] == 6);
    s.print(i[1]);
    s.assert(i[1] == 7);

    return i[1];}
}

