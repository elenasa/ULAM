#:R.ulam
  ulam 2;
element R {
  Bool first, second;
  Int test(){
    System s;

    Eltypo elf; //transient
    elf.t.func(5u); //dm set
    s.print(elf.t.bu); //==5u
    s.assert(elf.t.bu==5u);

    if(elf as Typo(1))
    {
      if(elf as Bar){
	elf.func(3u);
	s.print(elf.bu);
	first = (elf.bu == 3); //3u, from 7u
	s.assert(first);
      }
      else
	s.assert(false);

      s.print(elf.bu);
      second = (elf.bu == 3); //3u, from 7u
      s.assert(second);
      s.assert(first);
    }
    return elf.t.bu; //5u
  }
}

#:Typo.ulam
  ulam 1;
use Bar;
quark Typo(Int a) : Bar{
}

#:Bar.ulam
  ulam 1;
quark Bar
{
  Unsigned(3) bu = 7u;
  Void func(Unsigned u)
  {
    bu = (Unsigned(3)) u;
  }
}

#:Eltypo.ulam
  ulam 2;
transient Eltypo : Typo(1) {
  Typo(1) t;
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
