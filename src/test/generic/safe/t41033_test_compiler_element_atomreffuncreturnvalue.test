    return arg2;
  }

  Void caller1()
  {
    Foo f1;
    caller2(f1.atomof, false);
  }

  Atom & caller2(Atom& a1, Bool barg)
  {
    Foo f2;
    Atom & r2 = f2.atomof;
    return func(r2, a1, barg);  // Safe or not?  Can't know til runtime.
  }

  Int test() {
    System s;
    caller1();

    Atom f3;
    Atom & r = caller2(f3, false);
    s.assert(!(r is Foo));
    return 0;
  }
}

