#>P.ulam
  ulam 1;
use Q;
element P {
  Bool b;
  Int test() {
    SystemU3 s;
    s.print(__CLASS__);
    s.print(__CLASS_SIGNATURE__);
    s.print(__CLASS_PRETTY__);
    s.print(__CLASS_SIMPLE__);
    s.print(__CLASS_MANGLED__);
    s.assert(__CLASS_PRETTY__.lengthof == 1);
    s.assert(__CLASS_MANGLED__.lengthof==11);

    Q(4, 2) z;
    s.assert(z.sizeof == 2);
    z.printclassnames();

    Q() q; //ok
    s.assert(q.sizeof == 1);
    q.printclassnames();

    Q(4) w; //ok, 2nd arg is default
    s.assert(w.sizeof == 1);
    w.printclassnames();

    return (Int) (w.sizeof + q.sizeof + z.sizeof);
  }
}

#:Q.ulam
  ulam 1;
quark Q(Int b = 3, Unsigned(b) s = 1u) {
  typedef Unsigned(s) Foo;
  Foo f;
  Void printclassnames()
  {
    SystemU3 sq;
    sq.print(__CLASS__);
    sq.assert(__CLASS__.lengthof==1);

    sq.print(__CLASS_SIGNATURE__);
    sq.print(__CLASS_SIGNATURE__.lengthof);
    sq.assert(__CLASS_SIGNATURE__.lengthof==22);


    sq.print(__CLASS_PRETTY__);
    sq.print(__CLASS_PRETTY__.lengthof);
    sq.assert(__CLASS_PRETTY__.lengthof==27);

    //start count with [0]
    sq.assert(__CLASS_PRETTY__[5]==' ');
    sq.assert(__CLASS_PRETTY__[21]==__CLASS_PRETTY__[5]);
    sq.assert(__CLASS_PRETTY__[7]=='=');
    sq.assert(__CLASS_PRETTY__[23]==__CLASS_PRETTY__[7]);

    sq.print(__CLASS_SIMPLE__);
    sq.print(__CLASS_SIMPLE__.lengthof);
    sq.assert(__CLASS_SIMPLE__.lengthof==7);

    sq.print(__CLASS_MANGLED__);
    sq.print(__CLASS_MANGLED__.lengthof);
    sq.assert(__CLASS_MANGLED__.lengthof==28);
  }
}

#:SystemU3.ulam
  ulam 3;
quark SystemU3 : System {
  Void print(String s) native; //String(index) Arg:
