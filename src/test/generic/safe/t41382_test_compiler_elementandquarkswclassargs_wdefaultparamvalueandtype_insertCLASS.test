#>P.ulam
  ulam 1;
use Q;
element P {
  Bool b;
  Int test() {
    SystemU3 s;
    s.print(__CLASS__);
    s.print(__CLASSSIGNATURE__);
    s.print(__CLASSPRETTY__);
    s.print(__CLASSSIMPLE__);
    s.print(__CLASSMANGLED__);

    String nameP = __CLASSPRETTY__;
    s.assert(nameP.lengthof == 1);
    String mangledP = __CLASSMANGLED__;
    s.print(mangledP.lengthof);
    s.assert(mangledP.lengthof==11);

    Q(4, 2) z;
    s.assert(z.sizeof == 2);
    z.printclassnames();

    Q() q; //ok
    s.assert(q.sizeof == 1);
    q.printclassnames();

    Q(4) w; //ok, 2nd arg is default
    s.assert(w.sizeof == 1);
    w.printclassnames();

    return (Int) (w.sizeof + q.sizeof + z.sizeof);
  }
}

#:Q.ulam
  ulam 1;
quark Q(Int b = 3, Unsigned(b) s = 1u) {
  typedef Unsigned(s) Foo;
  Foo f;
  Void printclassnames()
  {
    SystemU3 sq;
    sq.print(__CLASS__);
    sq.print(__CLASSSIGNATURE__);
    String signaQ = __CLASSSIGNATURE__;
    sq.print(signaQ.lengthof);
    sq.assert(signaQ.lengthof==26);

    sq.print(__CLASSPRETTY__);
    String prettyQ = __CLASSPRETTY__;
    sq.print(prettyQ.lengthof);
    sq.assert(prettyQ.lengthof==31);
    sq.assert(prettyQ[9]==' ');
    sq.assert(prettyQ[25]==' ');
    sq.assert(prettyQ[11]=='=');
    sq.assert(prettyQ[27]=='=');

    sq.print(__CLASSSIMPLE__);
    String simpleQ = __CLASSSIMPLE__;
    sq.print(simpleQ.lengthof);
    sq.assert(simpleQ.lengthof==7);

    sq.print(__CLASSMANGLED__);
    String mangledQ = __CLASSMANGLED__;
    sq.print(mangledQ.lengthof);
    sq.assert(mangledQ.lengthof==28);


  }
}

#:SystemU3.ulam
  ulam 3;
quark SystemU3 : System {
  Void print(String s) native;
}

#:System.ulam
  ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
