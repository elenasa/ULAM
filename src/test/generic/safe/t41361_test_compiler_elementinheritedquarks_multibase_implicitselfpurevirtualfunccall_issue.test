    return 0;
  }

  @Override
  Void updateForSwapAll(Atom& obj,Bool force) {
    self.BondoFrom.updateForSwap(obj,force);
    self.BondoTo.updateForSwap(obj,force);
  }

  @Override
  Int couldSwapAll(Atom& obj) {
