#>Foo.ulam
  ulam 3;
element Foo {
  Bool first;
  Int sint;
  Int cint;
  Int test() {
    System s;
    Soo su;

    Int sfoo = su.foo(0); //see how virtual works
    s.print(sfoo);
    s.assert(sfoo == 40);

    Coo& cref = su;
    Int cfoo = cref.foo(1); //see how virtual works
    s.print(cfoo);
    s.assert(cfoo == 40);

    //custom array, aref for reading as virtual..
    sint = su[0];
    s.print(sint);
    s.assert(sint == 4);

    cint = cref[1];
    s.print(cint);
    s.assert(cint == 4);

    //custom array, aref for writing as virtual..TBD!!

    return 0;
  }
}

#:Soo.ulam
  ulam 3;
transient Soo : Coo {
  Unsigned(3) us = 4u;
  virtual Int aref(Int index)
  {
    return (Int) us; //index ignored
  }
  virtual Int foo(Int index)
  {
    return 40;
  }
}

#:Coo.ulam
  ulam 3;
quark Coo {
  Bool(3) cb = true;
  virtual Int aref(Int index)
  {
    return 12; //index ignored
  }
  virtual Int foo(Int index)
  {
    return 120;
  }
}

