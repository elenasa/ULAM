#>Foo.ulam
  ulam 1;
use Bar;
use System;
element Foo {
  System s;
  Unary(4) m_i;
  Unary(4) m_j;
  Bits(4) m_bits;
  Int(4) m_k;
  Bar m_bar;
  Bar check(Int v) {
    Bar b;
    b.val_b[1] = true;
    return b; }
  Int test() {
    m_bar = check(1);
    m_i = (Unary(4))((Int) m_bar);
    m_j = (Unary(4)) 7;
    m_bits = m_i;
    m_k = (Int(4)) m_bits;
    s.print((Unsigned) m_i);
    s.print((Unsigned) m_j);
    s.print((Unsigned) m_bits);
    s.print((Unsigned) m_k);
    return m_i;}
}

