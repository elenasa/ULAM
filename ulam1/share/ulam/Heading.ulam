ulam 1;
use EventWindow;
use Empty;

quark Heading {
  // Types
  typedef Unsigned(6) SiteNum;
  typedef Int(4) Delta;
  //  typedef Unsigned(3) Symmetry;

  // Services
  EventWindow ew;

  // State
  SiteNum target;
  Delta xoff, yoff;

  Void set(SiteNum vector) {
  }
  Bool move(SiteNum from, SiteNum to) {
    //    Symmetry old =
    Unsigned(3) old =
      ew.changeSymmetry((Unsigned(3)) 0);
    ew.swap(from,to);
    ew.changeSymmetry(old);
    return false;
  }
}
