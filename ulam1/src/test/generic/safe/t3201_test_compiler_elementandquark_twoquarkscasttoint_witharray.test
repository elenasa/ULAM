#>Foo.ulam
ulam 1;
use System;
use Bar;
element Foo {
System s;
Bar bar1;
Bar bar2;
Int(4) i[2];
Int test(){
Int(2) d;
d = 1;
bar1.x = (Unsigned(3)) d;
bar1.y = 2;
bar2.x = 3;
bar2.y = 0;
i[0] = (Int(4)) bar1;
i[1] = (Int(4)) bar2;
s.print(bar1.x);
s.print(bar1.y);
s.print(bar2.x);
s.print(bar2.y);

s.print(i[0]);
s.print(i[1]);

return i[1];}
}

