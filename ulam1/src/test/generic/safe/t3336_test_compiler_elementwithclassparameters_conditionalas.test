#>Foo.ulam
  ulam 1;
use Counter4;
use System;
//element Foo : Counter4(Foo.v) {
//element Foo : Counter4(v) {
element Foo : Counter4(2+1) {
  System s;
  Int e;
  constant Int v = 3;
  Int test(){
    Atom a;
    Foo f;
    a = f;
    if(a as Counter4(v))
    {
      a.incr();
      e = a.get();
      s.print(e);
    }
    else
      e=7;
    return e;
  }
}

#:Counter4.ulam
  ulam 1;
quark Counter4(Int x) {
  Int(x) d;
  Void incr(){
    d+=1;
  }
  Int get(){
    return d;
  }
}
