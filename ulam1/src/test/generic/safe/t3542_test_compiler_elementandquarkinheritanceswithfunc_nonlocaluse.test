##
## element and quark inheritances, regular (non-template), with func
## informed by t3541
##
#>Foo.ulam
  ulam 1;
use Basel;
use System;
element Foo : Basel {
  System s;
  Bool first;
  Int test() {
    s.print(su.cb);
    s.assert(su.cb);
    first = su.cb; //DM
    s.print(first);
    s.assert(first);
    first = su.check();
    s.print(first);
    s.assert(first);

    if(su.check())
    {
      s.print(su.lu);
      s.assert(su.lu == -1);
      su.lu = 1u;
      s.print(su.lu);
      s.assert(su.lu == 1);
      s.print(Foo.sizeof);
      s.assert(Foo.sizeof == 13);
      return Foo.sizeof;
    }
    return -1;
  }
}

#:Basel.ulam
  ulam 1;
use Casel;
quark Basel : Casel {
  Bool ba;
}

#:Casel.ulam
  ulam 1;
use Soo;
quark Casel {
  Soo su;
}

#:Soo.ulam
  ulam 1;
use Coo;
quark Soo : Coo {
  Unsigned(3) us = 4u;
}

#:Coo.ulam
  ulam 1;
use Loo;
quark Coo : Loo {
  Bool(3) cb = true;
  Bool check()
  {
    return cb;
  }
}

#:Loo.ulam
  ulam 1;
quark Loo {
  Int(5) lu = -1;
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}

