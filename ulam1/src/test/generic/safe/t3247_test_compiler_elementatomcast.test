##      // 'a has System' doesn't apply to eval because atoms have no class declarations; but testable for gencode
  use System;
element Foo {
  System s;
  Bool sp;
  Bool(3) bi, bh;
  Int d;
  Int test(){
    Atom a;
    Foo f;
    Bool b;
    b=a has System;
    s.assert(!b);
    a = f; //easy
    if(a is Foo)
      bi = true;
    b=a has System;
    s.assert(b);
    f = (Foo) a; //make sure a is a foo
    if(f has System)
      bh = true;
    d = (Int) (a has System) + 3;
    return d;
  }
}

