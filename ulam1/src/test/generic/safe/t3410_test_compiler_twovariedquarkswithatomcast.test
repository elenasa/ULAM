#>Foo.ulam
  ulam 1;
use IntXY;
use System;
element Foo {
  IntXY(1,4) q;
  IntXY(3u,4u) n;
  IntXY(2,3) p;
  Int test() {
    System s;

    //make into local quark
    IntXY(3u,4u) m = n.make(1,2);
    s.print(m.x);
    s.print(m.y);
    s.print(n.x);
    s.print(n.y);

    // local element from data member func
    Foo f = (Foo) n.fromXY(2,3);
    s.print(f.n.x);
    s.print(f.n.y);
    s.print(n.x);
    s.print(n.y);

    //make local element dm from local var quark
    f.n = m.make(4,1);
    s.print(f.n.x);
    s.print(f.n.y);
    s.print(m.x);
    s.print(m.y);

    /*
    //local element makeas using data member func
    Foo g = (Foo) n.makeAs(1,1);
    s.print(g.n.x);
    s.print(g.n.y);
    s.print(n.x);
    s.print(n.y);
    */
    return 0;
  }
}

