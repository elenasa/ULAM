  ulam 1;
use System;
use Car;
element Foo {
  typedef Car Pop[2];
  System s;
  Bool(3) b, c;
  Car sbar;
  Bool check(Int v) {
    return true;
  }
  Int test() {
    Pop m_bar2;
    Unary(2) i = 0;

    b = m_bar2[i].cb; //read case

    s.print(b); //true, should be
    s.assert(b); //true, should be

    m_bar2[i].cb = false; //write case

    c = m_bar2[i].cb; //read case

    s.print(c); //false
    s.assert(!c);

    return 0;
  }
}

