##      //informed by 3416 only return is Int("3") instead of atom; using class args for bitsize "3"
##      // (don't have to use native aref).
#>IntArrayTest.ulam
ulam 1;
use IAN;
 use System;
 element IntArrayTest {
constant Int isn = 3;
 typedef Int(isn) Intn;
 IAN(isn) ia;
  Intn func() {
 return ia[1];
 }
 Int test() {
System s;
 Intn a;
 a = func();
 s.print(a);
 s.assert(a == 2);
 return a;
  }
}

