use Soo;
use System;
element Foo : Soo {
  System s;
  Bool first;
  Int test() {
    Foo f;
    if(f as Coo)
    {
      first = f.cb;
      s.print(first);
      s.assert(first);
      s.print(f.sizeof);
      s.assert(f.sizeof == 3);
      return f.sizeof;
    }
    s.assert(false);
    return 0;
  }
}

